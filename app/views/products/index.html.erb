<h1>Products#index</h1>
<p>Find me in app/views/products/index.html.erb</p>



<table class="table">
	<thead>
		<tr>
			<th>ID</th>
			<th>Name</th>
			<th>SKU</th>
			<th>View</th>
			<th>Edit</th>
		</tr>
	</thead>
	<tbody>
		<% @products.each do |obj|%>
		<tr>
			<td><%= obj.id %></td>
			<td><%= obj.name %></td>
			<td><%= obj.sku %></td>
			<td><%= link_to "View", product_path(obj) %></td>
			<td><%= link_to "Edit", edit_product_path(obj) %></td>
		</tr>
		<% end %>
	</tbody>
</table>
